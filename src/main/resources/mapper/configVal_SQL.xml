<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="configVal">	

		
	<select id="selConfigVal" resultType="hashmap">
		select * from configval;
	</select>
	
	
	<update id="setConfigVal" parameterType="hashmap">
		Update configVal
		Set confrssi = #{confrssi}, managerssi= #{managerssi}, rasp_id =  #{rasp_id} , rasp_pass =  #{rasp_pass};
		
	</update>
	
</mapper>

